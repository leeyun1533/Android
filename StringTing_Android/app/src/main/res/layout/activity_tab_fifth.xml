<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:id="@+id/sb1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="51.1dp"
                    android:layout_height="19dp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="17.5dp"
                    android:text="내 정보 "
                    android:textSize="17.5dp" />

            </RelativeLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_below="@+id/sb1"
            android:layout_marginTop="35.5dp"
            android:layout_alignParentEnd="true">

            <ImageView
                android:id="@+id/imageView18"
                android:layout_width="105dp"
                android:layout_height="105dp"
                android:layout_gravity="center"
                android:layout_marginBottom="20dp"
                app:srcCompat="@drawable/circle" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="285dp"
                android:layout_height="16.5dp"
                android:layout_gravity="center"
                android:text="내 프로필 보기 및 수정"
                android:layout_marginBottom="18.89dp"
                android:textSize="15dp"
                android:textAlignment="center" />

            <TextView
                android:id="@+id/textView11"
                android:layout_width="74.5dp"
                android:layout_height="12.3dp"
                android:layout_gravity="center"
                android:gravity="center"
                android:textSize="11dp"
                android:text="코인 충전"
                android:layout_marginBottom="30dp"
                />



            <View
                android:layout_width="393dp"
                android:layout_height="3.3dp"
                android:background="#cecece"
                android:layout_gravity="center"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:layout_marginTop="30dp"
                android:orientation="horizontal"
                >



                <ImageButton
                    android:id="@+id/textView16"
                    android:layout_width="120dp"
                    android:layout_height="120dp"
                    android:gravity="center"
                    android:textAlignment="center"
                    android:src="@drawable/xcopy43x"
                   />

                <ImageButton
                    android:id="@+id/textView17"
                    android:layout_width="120dp"
                    android:layout_height="120dp"
                    android:gravity="center"
                    android:textAlignment="center"
                    android:src="@drawable/xcopy63x"
                     />

                <ImageButton
                    android:id="@+id/textView18"
                    android:layout_width="120dp"
                    android:layout_height="120dp"
                    android:gravity="center"
                    android:background="@drawable/border"
                    android:textAlignment="center"
                    android:src="@drawable/xcopy73x"
                   />

            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:layout_marginTop="30dp"
                android:orientation="horizontal"
                >



                <ImageButton
                    android:id="@+id/textView19"
                    android:layout_width="120dp"
                    android:layout_height="120dp"
                    android:layout_gravity="left"
                    android:src="@drawable/xcopy53x"
                    />



            </LinearLayout>

        </LinearLayout>

    </RelativeLayout>

</FrameLayout>
